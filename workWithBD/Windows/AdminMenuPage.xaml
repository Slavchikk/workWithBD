<Page x:Class="workWithBD.Windows.AdminMenuPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:workWithBD.Windows"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AdminMenuPage">

    <Grid>
        <StackPanel Orientation="Horizontal" VerticalAlignment="Top" Margin="0,30,3,0">
        <!--<Image Source="workWithBD\Photo\images.jpg" Margin="40,30,650,310" Height="100" Width="100"></Image>-->
            <TextBlock  Margin="30" Style="{StaticResource TBStyle}"  HorizontalAlignment="Center"  Name="AdminName">UserName</TextBlock>
        <Button Margin="30" Width="160"   Template="{StaticResource CTButon}" Click ="Btn_go_admin">Просмотр пользователей</Button>
            <Button  Margin="30" Width="160"   Template="{StaticResource CTButon}" Click ="Btn_go_list">Просмотр билетов</Button>
        </StackPanel>
    </Grid>
</Page>
