<Window x:Class="workWithBD.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:workWithBD"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
       
        <Grid.RowDefinitions>
            <RowDefinition ></RowDefinition>
            <RowDefinition Height="10*"></RowDefinition>
        </Grid.RowDefinitions>
        <Button Grid.Column="1" Width="100" HorizontalAlignment="Left" Click="Btn_Go_reg">Регистрация</Button>
        <StackPanel Grid.Row="1" Grid.ColumnSpan="2">
            <Frame Name="MainFrame" NavigationUIVisibility="Hidden"></Frame>
        </StackPanel>

    </Grid>
</Window>
